@{
    ViewBag.Title = "Logon.cshtml";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="logon-banner">
</div>
<div id="logon-container">
    <div id="logon-dialog">
        <input class="logon-input-userid" name="userid" title="Please enter a Password" maxlength="50"
            placeholder="username" type="text" />
        <input class="logon-input-password" name="password" title="Please enter a Password"
            maxlength="50" placeholder="password" type="password" />
        <label class="checkbox">
            <input class="logon-input-rememberme" name="rememberme" value="Y" type="checkbox" />
            Remember Me
        </label>
        <input id="logon-input-button" class="btn btn-primary " value="Log In" type="button" />
    </div>
</div>
